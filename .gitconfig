[user]
	name = maxpushka
[init]
	defaultBranch = main
[core]
	editor = nvim
[alias]
	a = add
	aa = add --all
	b = branch
	c = commit
	cl = clone --recurse-submodules
	co = checkout
	d = diff
	f = fetch
  fa = fetch --all
	lg = log --stat --graph
	p = push
  pf = push --force
	rb = rebase
	rs = restore
	rss = restore --staged
	st = status
	sw = switch
	up = pull --rebase
	wa = worktree add
	wl = worktree list
	wm = worktree move
	wrm = worktree remove
